/*----------- button mobilemenu -------------*/
.mobilemenu span.ttclose,.mobilemenu span.ttopen {
  position: absolute;
  top: 0px;
  right: 0;
  padding: 5px 0;
}
.mobilemenu span.ttclose a {
  border: 0 none;
  color: #333333;
  height: 30px;
  padding: 0;
  width: 30px;
  line-height: 30px;
  text-align: center;
}
.mobilemenu span.ttclose a:before {
  content: "\f107";
  font-family: "Font Awesome 5 Free";
  font-weight: 600;
}
.mobilemenu span.ttopen a {
  border: 0 none;
  display: block;
  height: 30px;
  padding: 0;
  width: 30px;
  line-height: 30px;
  text-align: center;
}
.mobilemenu span.ttopen a:before {
  content: "\f106";
  font-family: "Font Awesome 5 Free";
  font-weight: 600;
}
.mobilemenu span.no-close a,.mobilemenu ul li ul span.ttclose a { background:none; }
.mobilemenu span.up a { 
  background: url("{{ 'menu_up.png' | asset_url }}") no-repeat scroll 50% 50%;
    }
/*.mobilemenu span.head a:hover*/
.mobilemenu li.active>span.ttclose a {
  background: url({{ 'menu_up.png' | asset_url }}) no-repeat 50% 50%;
}
.mobilemenu li.selected span.ttclose a {
  background: url({{ 'menu_up.png' | asset_url }}) no-repeat 50% 50%;
}
.menu-mobile {
  display: inline-block;
  width: 100%;
