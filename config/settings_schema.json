        "options": [
          {
            "value": "3:2",
            "label": "3:2 - Landscape"
          },
          {
            "value": "4:3",
            "label": "4:3 - Landscape"
          },
          {
            "value": "1:1",
            "label": "1:1 - Square"
          },
          {
            "value": "3:4",
            "label": "3:4 - Portrait"
          },
          {
            "value": "2:3",
            "label": "2:3 - Portrait"
          }
        ],
        "default": "1:1"
