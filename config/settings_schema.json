      {
        "type": "checkbox",
        "id": "product_reviews_enable",
        "label": "Enable product reviews",
        "info": "Install the free Shopify [Product Reviews](https:\/\/apps.shopify.com\/product-reviews) app to add product reviews.",
        "default": true
      },
      {
        "type": "select",
        "id": "product_reviews_app",
        "label": "Select the review app that you're using?",
        "options": [
          {
            "value": "shopifyreviews",
            "label": "Shopify Product Reviews App"
          },
          {
            "value": "loox",
            "label": "Loox"
          },
          {
            "value": "alireviews",
            "label": "Ali Reviews by FireApps"
          },
          {
            "value": "judgeme",
            "label": "Judge.me"
          },
          {
            "value": "stamped",
            "label": "Stamped.io"
          },
          {
            "value": "yotpo",
            "label": "Yotpo"
          }
        ],
        "default": "shopifyreviews",
        "info": "One - [Shopify Product Reviews App](https:\/\/apps.shopify.com\/product-reviews) Two - [Loox Product Reviews App](https:\/\/apps.shopify.com\/loox) Three - [Ali Product Reviews App](https:\/\/apps.shopify.com\/ali-reviews) Four - [Judge.me Product Reviews](https:\/\/apps.shopify.com\/judgeme) Five - [Stamped Product Reviews](https:\/\/apps.shopify.com\/product-reviews-addon)"
      },
      {
        "type": "checkbox",
        "id": "grid_vendor_enable",
        "label": "Show brand names on product grid",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "hide_deal",
        "label": "Hide deal info on product grid",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "hide_wishlist",
        "label": "Hide Wishlist option",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "hide_quick_view",
        "label": "Hide Quickview option",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "search_autofil_enable",
        "label": "Enable Search Autofill",
        "default": true
      },
      {
        "type": "select",
        "id": "add_to_cart_layout",
